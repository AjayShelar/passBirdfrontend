{
  "auth": {
    "passBirdAuth": {
      "service": "Cognito",
      "providerPlugin": "awscloudformation",
      "dependsOn": [],
      "customAuth": false,
      "frontendAuthConfig": {
        "socialProviders": [],
        "usernameAttributes": [
          "EMAIL"
        ],
        "signupAttributes": [
          "EMAIL"
        ],
        "passwordProtectionSettings": {
          "passwordPolicyMinLength": 8,
          "passwordPolicyCharacters": [
            "REQUIRES_SYMBOLS"
          ]
        },
        "mfaConfiguration": "ON",
        "mfaTypes": [
          "SMS"
        ],
        "verificationMechanisms": [
          "PHONE_NUMBER"
        ]
      }
    }
  }
}